<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<Nullable>enable</Nullable>
		<Version>1.0.0</Version>
		<Authors>Khaled Md Tuhidul Hossain</Authors>
		<Company>EncryptedTouhid</Company>
		<Description>A middleware to limit CPU usage for ASP.NET Core applications. Effortless CPU Usage Control for ASP.NET Core Applications.</Description>
		<PackageTags>cpu limit middleware aspnetcore throttling performance</PackageTags>
		<RepositoryUrl>https://github.com/encryptedtouhid/CpuGuard.NET</RepositoryUrl>
